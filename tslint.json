{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-react",
        "tslint-eslint-rules",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "no-unused-expression": false,
        "no-duplicate-imports": false,
        "no-submodule-imports": false
    },
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    }
}